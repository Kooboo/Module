<?xml version="1.0" encoding="utf-8"?>
<ModelBase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>k-omit</Name>
  <DisplayName>Omit the element open tag.</DisplayName>
  <Example>
    <![CDATA[ 
    <div>
       <p k-label='datakey' k-omit=true> my vlaue </p> 
    </div>
    
    
    <!-- output -->
    <div>
       my vlaue 
    </div> 
    ]]>
  </Example>
</ModelBase>