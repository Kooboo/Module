<?xml version="1.0" encoding="utf-8"?>
<KScriptSetting>
  <Name>Dictionary</Name>
  <Description>Item Collections of forms, query string or headers... </Description>
  <Example>

    var value = items.fieldname; 
    // or
    var value = items["fieldname"]; 
    
  </Example>
  <Props>
    <Property>
      <Name>keys</Name>
      <Description>keys</Description>
      <Type>object</Type>
    </Property>
    <Property>
      <Name>values</Name>
      <Description>values</Description>
      <Type>object</Type>
    </Property>
    <Property>
      <Name>length</Name>
      <Description>length</Description>
      <Type>Int32</Type>
    </Property>
    <Property>
      <Name>item</Name>
      <Description>item</Description>
      <Type>string</Type>
    </Property>
  </Props>
  <Methods>
    <Method>
      <Name>get</Name>
      <Description>get value form the collection.</Description>
      <Example>
        
      var value = items.get("fieldname");
      
      </Example>
      <Params>
        <Param>
          <Name>key</Name>
          <Description>The data key to get value</Description>
          <Type>string</Type>
        </Param>
      </Params>
      <ReturnType>string</ReturnType>
    </Method>
    <Method>
      <Name>add</Name>
      <Description>add new value into the collection</Description>
      <Example>
      
      items.add("key", "value"); 
      
      </Example>
      <Params>
        <Param>
          <Name>key</Name>
          <Description>data key</Description>
          <Type>string</Type>
        </Param>
        <Param>
          <Name>value</Name>
          <Description>value</Description>
          <Type>string</Type>
        </Param>
      </Params>
    </Method>
    <Method>
      <Name>contains</Name>
      <Description>check whether the collection has that key or not.</Description>
      <Example>
      
        if (items.contains("key"))
        {
        // has the key.
        }
      
      </Example>
      <Params>
        <Param>
          <Name>key</Name>
          <Description>data key</Description>
          <Type>object</Type>
        </Param>
      </Params>
      <ReturnType>boolean</ReturnType>
    </Method>
    
  </Methods>
</KScriptSetting>